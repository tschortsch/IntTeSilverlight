        <sdk:Label Height="28" HorizontalAlignment="Left" Margin="252,36,0,0" Name="label1" VerticalAlignment="Top" Width="120" Content="Game" />
        <sdk:Label Height="28" HorizontalAlignment="Left" Margin="326,134,0,0" Name="playerLabel" VerticalAlignment="Top" Width="120" Content="{Binding Name}" />
    </Grid>
</navigation:Page>
